pipeline {
    agent any

    parameters {
        string(name: 'BRANCH_NAME', defaultValue: 'master', description: 'Branch a ser buildada')
    }

    stages {
        stage("Current Branch") {
            steps {
                echo "Building branch $BRANCH_NAME"
            }
        }

        stage("Build") {
            steps {
                echo "Executando build..."
            }
        }

        stage("Dockerize") {
            steps {
                echo "Executando containerização..."
            }
        }


    }
}